{
    "manifest_version": 3,
    "name": "PR reviewer",
    "description": "Auto review github PR",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "github-logo.png"
    },
    "permissions":["tabs", "scripting"],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ]
  }